# MLflow Configuration
MLFLOW_KSERVE_MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_KSERVE_MLFLOW_WEBHOOK_SECRET=your-webhook-secret-here
MLFLOW_KSERVE_MLFLOW_WEBHOOK_URL=http://localhost:8000/webhook
MLFLOW_KSERVE_MLFLOW_WEBHOOK_NAME=mlflow-kserve-webhook

# Storage Configuration
MLFLOW_KSERVE_STORAGE_URI_BASE=gs://your-bucket-name

# Kubernetes Configuration (optional)
MLFLOW_KSERVE_KUBE_NAMESPACE=kserve-mlflow-models
MLFLOW_KSERVE_KUBE_IN_CLUSTER=false

# Server Configuration (optional)
MLFLOW_KSERVE_APP_HOST=0.0.0.0
MLFLOW_KSERVE_APP_PORT=8000

# Template Configuration (optional)
MLFLOW_KSERVE_INFERENCE_SERVICE_TEMPLATE=templates/inference_service.yaml.j2

# Resource Limits (optional)
MLFLOW_KSERVE_PREDICTOR_CPU_REQUEST=100m
MLFLOW_KSERVE_PREDICTOR_CPU_LIMIT=1
MLFLOW_KSERVE_PREDICTOR_MEMORY_REQUEST=512Mi
MLFLOW_KSERVE_PREDICTOR_MEMORY_LIMIT=2Gi

# Logging (optional)
MLFLOW_KSERVE_LOG_LEVEL=INFO
